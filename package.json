{
  "name": "@zoom/videosdk-react",
  "version": "0.0.1",
  "description": "A library for using @zoom/videosdk with React",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/zoom/videosdk-react",
  "bugs": {
    "url": "https://github.com/zoom/videosdk-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoom/videosdk-react.git"
  },
  "author": "Ekaansh Arora <ekaansh.arora@zoom.us>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "oxlint --type-aware",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && bun publish",
    "prepublishOnly": "bun run build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jsrsasign": "^10.5.15",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.3",
    "@zoom/videosdk": "^2.3.5",
    "bumpp": "^10.3.1",
    "happy-dom": "^20.0.8",
    "jsrsasign": "^11.1.0",
    "oxlint": "^1.24.0",
    "oxlint-tsgolint": "^0.3.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsdown": "^0.14.2",
    "typedoc": "^0.28.14",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.1.19",
    "vitest": "^4.0.3"
  },
  "dependencies": {
    "dequal": "^2.0.3"
  },
  "peerDependencies": {
    "@zoom/videosdk": "^2.3.5",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}