{
  "name": "@zoom/videosdk-react",
  "version": "0.0.1",
  "description": "A library for using @zoom/videosdk with React",
  "type": "module",
  "homepage": "https://github.com/zoom/videosdk-react",
  "bugs": {
    "url": "https://github.com/zoom/videosdk-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoom/videosdk-react.git"
  },
  "author": "Ekaansh Arora <ekaansh.arora@zoom.us>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test:unit": "vitest src/",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "vitest src/ && playwright test",
    "lint": "oxlint --type-aware",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && bun publish",
    "prepublishOnly": "bun run build",
    "docs": "typedoc"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@tailwindcss/vite": "^4.1.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jsrsasign": "^10.5.15",
    "@types/node": "^24.9.1",
    "@types/pngjs": "^6.0.5",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.3",
    "@zoom/videosdk": "^2.3.5",
    "bumpp": "^10.3.1",
    "dotenv": "^17.2.3",
    "happy-dom": "^20.0.8",
    "jsrsasign": "^11.1.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "pngjs": "^7.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsdown": "^0.19.0",
    "typedoc": "^0.28.14",
    "typedoc-plugin-mdn-links": "^5.0.10",
    "typedoc-plugin-missing-exports": "^4.1.2",
    "typescript": "^5.9.3",
    "vite": "npm:rolldown-vite@^7.3.1",
    "vitest": "^4.0.3"
  },
  "peerDependencies": {
    "@zoom/videosdk": "^2.3.5",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}