{
  "name": "@zoom/videosdk-react",
  "version": "0.0.1",
  "description": "A library for using @zoom/videosdk with React",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/zoom/videosdk-react",
  "bugs": {
    "url": "https://github.com/zoom/videosdk-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoom/videosdk-react.git"
  },
  "author": "Ekaansh Arora <ekaansh.arora@zoom.us>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "playground": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jsrsasign": "^10.5.15",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "bumpp": "^10.1.0",
    "happy-dom": "^17.4.6",
    "jsrsasign": "^11.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsdown": "^0.14.2",
    "typescript": "^5.8.3",
    "vite": "npm:rolldown-vite@^7.1.5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@zoom/videosdk": "^2.2.10",
    "dequal": "^2.0.3"
  },
  "peerDependencie": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide"
  ]
}